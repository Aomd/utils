{
    "compilerOptions": {
        "target":"es6",
        "module": "commonJS",
        "checkJs": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "src/**/*",
        "node_modules/@types/**/*"
    ],
    "path":{
        "@/*":["./src/*"]
    }
}